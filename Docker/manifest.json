{
    "name": "auto-qa",
    "label": "auto_QA",
    "description": "This gear script runs all the functions to test ACR phantom",
    "version": "0.2.9",
    "author":  "Xuchu Liu",
    "maintainer":  "Jie",
    "cite":  "Don’t repeat yourself.",
    "license": "Apache-2.0",
    "url": "https://www.reinhand.com",
    "source":  "https://github.com/Achillesy/gear_QA",

    "environment": {
        "FLYWHEEL":"/flywheel/v0"
     },
           
    "custom": {
        "gear-builder": {
            "category": "analysis",
            "image": "ahsoka/matlab-mcr-python:0.1.0"
        },
            
        "flywheel": {
            "suite": "auto QA"
         },

         "generator": "来来我是一块菠萝，箩箩箩箩箩箩箩箩箩箩箩箩箩箩箩箩"
    },
    
    "inputs": {
        "input_file": {
            "description": "the dicom file you'll auto QA (must include SAG LOC and Axial T1 Image)",
            "base": "file",
            "type": { "enum": [ "dicom" ] }
        },
        "output_file": {
            "description": "the excel file you'll write the result",
            "base": "file",
            "type": { "enum": [ "text" ] }
        }
    },

    "config": {
        "vendor": {
            "description": "GE or Siemens Scanner? GE=1, Siemens=0: ",
            "default": 1,
            "mininum": 0,
            "maxinum": 1,
            "type": "integer"
        }
    }
}
